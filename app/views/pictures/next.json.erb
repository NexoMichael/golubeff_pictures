{
  "firstId" : "<%= @pictures.first.id %>",
  "lastId" : "<%= @pictures.last.id %>",
  "images" : [
    <% @pictures.each do |picture| %>
      {
        "imageId" : "<%= picture.id %>",
        "imagePreview" : "<%= picture.url(:thumb) %>",
        "imageFullSize" : "<%= picture.url(:full) %>",
        "source" : "<%= picture.referral_url %>" 
      }
      <% if picture != @pictures.last %>,<% end %>
    <% end %>
  ]
}
